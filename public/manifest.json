{
  "manifest_version":3,

  "name":"extension-zed",
  "description":"from a to z",
  "version":"1.0.0",

  "action":{
    "default_popup":"index.html",
    "default_title":"hi ya khalid",
    "default_icon": "images/icon.png"
  },

  "background": {
    "service_worker": "background.js"
  },

  "permissions":[
    "scripting",
    "activeTab",
    "storage"
  ],
  "host_permissions":[
    "https://*/*",
    "http://*/*"
  ],

"commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Z"
      }
    }
  }
}
